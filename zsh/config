LC_CTYPE=en_AU.UTF-8
EDITOR="vim"

GEMDIR=`gem env gemdir`
RUBYOPT=rubygems

HISTFILE=~/.zsh_history
SAVEHIST=10000
HISTSIZE=10000

bindkey -v

REPORTTIME=10

autoload compinit
compinit

unsetopt auto_name_dirs
[[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"  # This loads RVM into a shell session.
