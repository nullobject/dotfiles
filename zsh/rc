eval "$(rbenv init -)"
eval "$(nodenv init -)"

. ~/.zsh/paths
. ~/.zsh/prompt
. ~/.zsh/aliases
. ~/.zsh/git_aliases
. ~/.zsh/functions

HISTFILE=~/.zsh_history
HISTSIZE=1000000
SAVEHIST=1000000
setopt extendedglob notify
setopt append_history
setopt inc_append_history
setopt prompt_subst
setopt hist_ignore_dups
bindkey -v

autoload -Uz compinit
compinit

export EDITOR=vi
